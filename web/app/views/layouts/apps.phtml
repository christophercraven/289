<div class="container">
    <h1>Latest Apps</h1>
    <ul class="app-listings">
    {% for app in apps %}
        <li>
		<div class="icon">
        <img src={{ app.icon_app }} />
		</div>
        <h2>{{ app.name_app }}</h2>
        <p>{{ app.tagline_app }}</p>
        <p data-desc={{ app.desc_app }}></p>
        <span>Votes:</span>
        </li>
    {% endfor %}

    </ul>
</div>